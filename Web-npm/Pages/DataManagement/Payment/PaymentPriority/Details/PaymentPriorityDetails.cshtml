@page "/datamanagement/payment/paymentpriority/details/{id:long}"
@model Web.Pages.DataManagement.Payment.PaymentPriority.Details.PaymentPriorityDetailsModel
@{
    ViewData["Title"] = "Payment Priority Details";
}
@section Scripts{
    <environment include="Development">
        <script src="/js/paymentPriorityDetails.js"></script>
        <script src="/js/site.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="/js/paymentPriorityDetails.min.js"></script>
        <script src="/js/site.min.js"></script>

    </environment>
}
    <div class="row justify-content-center" id="app" data-entity-id="@Model.EntityId">
        <c-section :title="details.PaymentPriorityId > 0 ? 'Payment Priority Details' : 'New Payment Priority'">
            <template v-slot:content>
                <div class="mt-5">

                <c-input input-label="Name: ">
                    <template v-slot:input>
                        <input class="bg-blue-900 border-2 rounded-full" type="text" v-model="details.Name" />
                    </template>
                </c-input>
                <c-button type="button" title="Save" v-on:click="save">Save</c-button>
                </div>
            </template>
        </c-section>
    </div>
