@page "/datamanagement/business/details/{id:long}"
@model Web.Pages.DataManagement.Business.Details.BusinessDetailsModel
@{
    ViewData["Title"] = "Business Details";
}
@section Scripts{
    <environment include="Development">
        <script src="/js/businessDetails.js"></script>
        <script src="/js/site.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="/js/businessDetails.min.js"></script>
        <script src="/js/site.min.js"></script>

    </environment>
}
    <div class="row justify-content-center" id="app" data-entity-id="@Model.EntityId">
        <div class="input-group text-center">
            <label>Name: </label>
            <input v-model="details.Name" />
        </div>
        <button type="button" class="btn btn-primary" v-on:click="save">Save</button>

    </div>
