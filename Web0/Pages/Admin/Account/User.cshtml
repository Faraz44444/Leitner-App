@page
@model Web0.Pages.Admin.Account.UserModel
@{
    ViewData["Title"] = Model.EntityName;
}
@section Scripts{
<environment>
    <script src="~/js/User.js" asp-append-version="false"></script>
</environment>
}

<div class="row justify-content-center" id="app">
    <div class="col">
        <table class="table">
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Email</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Last Login At</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <template v-for="item in items">
                        <td>{{item.Username}}</td>
                        <td>{{item.Email}}</td>
                        <td>{{item.FirstName}}</td>
                        <td>{{item.LastName}}</td>
                        <td>{{item.LastLoginAt}}</td>
                    </template>
                </tr>
            </tbody>
        </table>
    </div>
</div>